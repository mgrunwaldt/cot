
#googleMapsDiv{
    position: relative;
    width: 80%;
    margin-left: 10%;
    height: 50px;
    overflow: hidden;
}
#googleMapsButton{
    position: relative;
    width: 250px;
    margin: 0px auto;
    margin-top: 7px;
    border: 1px solid rgba(255,255,255,0.5);
    text-align: center;
    height: 34px;
    line-height: 34px;
    color: white;
    font-weight: bold;
    letter-spacing: 3px;
    font-size: 14px;
}
#googleMapsButton:hover{
    cursor:pointer;
    border:1px solid white;
}
#locationMaps{
    position: relative;
    width: 80%;
    margin-left: 10%;
    height: auto;
    background-color: #F2F2F2;
}
#locationGoogleMap{
    position: absolute;
    width: 100%;
    height:100%;
    top:0px;
    left:0px;
    opacity:0;
}
#locationMap{
    position: relative;
    width: 80%;
    margin: 10% 10% 10% 10%;
}
#map-canvas{
    width:100%;
    height:100%;
}