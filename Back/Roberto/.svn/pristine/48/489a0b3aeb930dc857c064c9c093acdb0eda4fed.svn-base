.centeredContent{
    width:90%;
    margin-left:5%;
    height:auto;
    overflow:hidden;
}
#headerMenuIcon{
    position: absolute;
    width: 35px;
    left: 0px;
    margin-top: 15px;
    top: 8px;
}
#headerDummy{
    height: 72px;
}
#header{
    position:fixed;
    height:70px;
}
#headerMenuLogo{
    position: relative;
    display: block;
    margin: 0 auto;
    height: 50px;
    margin-top: 10px;
    float:none;
}
#headerContactInfoButton{
    position: absolute;
    right: 0px;
    top: 30px;
}
#footerCol1{
    margin-top:10px;
    margin-left:0px;
    width: 46%;
    font-size: 8px;
    letter-spacing: 1.3px;
}
#footerCol3{
    margin-top:10px;
    max-width: 54%;
    float: right;
    margin-left: 0px;
    font-size: 8px;
}
#footer{
    height:60px;
}
#footerDummy{
    height:60px;
}
#menuDiv{
    position:fixed;
    height:0px;
    width:50%;
    z-index: 1;
    overflow:hidden;
}
#menuWrapper{
    position:relative;
    padding-bottom:20px;
    height:auto;
    overflow:hidden;
}
.mobileMenuItem{
    border-bottom: 1px solid white;
    position: relative;
    width: 99%;
    margin-top: 10px;
    color: white;
    font-size: 12px;
    letter-spacing: 2px;
    text-align: center;
    height: 24px;
    line-height: 17px;
}
#contactDiv{
    position:fixed;
    width: 99%;
    top: 70px;
}
#galleryTitle{
    font-size: 30px;
    letter-spacing: 7px;
    top: 130px;
}
.columnsText{
    -webkit-column-count: 1;
    -moz-column-count: 1;
    column-count: 1;
    width:90%;
    margin-left:5%;
}
.highlightImage{
    position: absolute;
    top: -9999px;
    bottom: -9999px;
    left: -9999px;
    right: -9999px;
    margin: auto;
    width:auto;
    min-width:100%;
    height:250px;
}
.infoContainer{
    top:29%;
}
.sierrasText{
    font-size:18px;
    letter-spacing:8px;
}
.highlightImageDiv{
    height:250px;
    max-height:250px;
    overflow:hidden;
}
.highlightCaptionText{
    font-size: 11px;
    width: 90%;
    margin-left: 5%;
    line-height: 20px;
}
.leftImg{
    width:100%;
}
.rightImg{
    width:100%;
    margin-left:0px;
}
.pairedImages{
    width:90%;
    margin-left:5%;
}